    <form action="?" method="POST">
        <div class="g-recaptcha" data-sitekey="6LfZXnAqAAAAAH4uatiSLyjlCc8vkd9dmYb9kAne" data-callback="captchaCallback"></div>
    </form>

    <script>
        function captchaCallback(response) {
            // Sends the reCAPTCHA token back to Flutter WebView
            window.Captcha.postMessage(response);
        }
    </script>
</body>
